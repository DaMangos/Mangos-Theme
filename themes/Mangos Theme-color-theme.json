{
    "name": "Mangos Theme",
    "type": "dark",
    "tokenColors": [
        {
            "name": "attributes",
            "scope": [
                "entity.other.attribute.",
                "punctuation.section.attribute."
            ],
            "settings": {
                "foreground": "#ffa150",
                "fontStyle": ""
            }
        },
        {
            "name": "comments",
            "scope": [
                "comment.",
                "comment.block.documentation. storage.",
                "punctuation.definition.comment."
            ],
            "settings": {
                "foreground": "#a3b0bf",
                "fontStyle": ""
            }
        },
        {
            "name": "comments c",
            "scope": [
                "comment.block.documentation.c storage.",
                "comment.block.documentation.c keyword.",
            ],
            "settings": {
                "foreground": "#a3b0bf",
                "fontStyle": ""
            }
        },
        {
            "name": "comments cpp",
            "scope": [
                "comment.block.documentation.cpp storage.",
                "comment.block.documentation.cpp keyword.",
            ],
            "settings": {
                "foreground": "#a3b0bf",
                "fontStyle": ""
            }
        },
        {
            "name": "decorator",
            "scope": [
                "entity.name.function.decorator.",
                "punctuation.definition.decorator.",
            ],
            "settings": {
                "foreground": "#ffa150",
                "fontStyle": ""
            }
        },
        {
            "name": "annotation",
            "scope": [
                "storage.type.annotation.",
                "punctuation.definition.annotation.",
            ],
            "settings": {
                "foreground": "#ffa150",
                "fontStyle": ""
            }
        },
        {
            "name": "default",
            "scope": [
                "",
                "keyword.operator.",
                "punctuation."
            ],
            "settings": {
                "foreground": "#e3e3e3",
                "fontStyle": ""
            }
        },
        {
            "name": "default cpp",
            "scope": [
                "storage.modifier.reference.cpp",
                "storage.modifier.pointer.cpp"
            ],
            "settings": {
                "foreground": "#e3e3e3",
                "fontStyle": ""
            }
        },
        {
            "name": "default java",
            "scope": [
                "keyword.control.ternary.java",
                "storage.type.function.arrow.java",
                "storage.type.generic.wildcard.java",
                "variable.language.wildcard.java"
            ],
            "settings": {
                "foreground": "#e3e3e3",
                "fontStyle": ""
            }
        },
        {
            "name": "default python",
            "scope": [
                "constant.character.format.placeholder.other.python"
            ],
            "settings": {
                "foreground": "#e3e3e3",
                "fontStyle": ""
            }
        },
        {
            "name": "functions",
            "scope": [
                "entity.name.function.",
                "meta.function-call",
                "support.function."
            ],
            "settings": {
                "foreground": "#a6d67f",
                "fontStyle": ""
            }
        },
        {
            "name": "keywords",
            "scope": [
                "constant.language.",
                "keyword.",
                "storage.",
                "variable.language."
            ],
            "settings": {
                "foreground": "#fe7bb2",
                "fontStyle": "bold"
            }
        },
        {
            "name": "keywords c",
            "scope": [
                "keyword.operator.sizeof.c"
            ],
            "settings": {
                "foreground": "#fe7bb2",
                "fontStyle": "bold"
            }
        },
        {
            "name": "keywords cpp",
            "scope": [
                "entity.name.operator.cpp",
                "keyword.operator.wordlike.cpp keyword.operator."
            ],
            "settings": {
                "foreground": "#fe7bb2",
                "fontStyle": "bold"
            }
        },
        {
            "name": "preprocessor",
            "scope": [
                "keyword.control.directive.",
                "punctuation.definition.directive.",
                "entity.name.function.preprocessor.",
                "entity.name.other.preprocessor.",
                "meta.preprocessor."
            ],
            "settings": {
                "foreground": "#ffa150",
                "fontStyle": ""
            }
        },
        {
            "name": "preprocessor c",
            "scope": [
                "meta.preprocessor.pragma.c",
                "meta.preprocessor.pragma.c entity.",
                "meta.preprocessor.macro.c",
                "meta.preprocessor.macro.c entity."
            ],
            "settings": {
                "foreground": "#ffa150",
                "fontStyle": ""
            }
        },
        {
            "name": "preprocessor cpp",
            "scope": [
                "meta.preprocessor.pragma.cpp",
                "meta.preprocessor.pragma.cpp entity.",
                "meta.preprocessor.macro.cpp",
                "meta.preprocessor.macro.cpp entity."
            ],
            "settings": {
                "foreground": "#ffa150",
                "fontStyle": ""
            }
        },
        {
            "name": "numbers",
            "scope": [
                "constant.numeric.",
                "keyword.other.unit."
            ],
            "settings": {
                "foreground": "#a89df7",
                "fontStyle": ""
            }
        },
        {
            "name": "strings",
            "scope": [
                "constant.",
                "punctuation.definition.string.",
                "string."
            ],
            "settings": {
                "foreground": "#fe8170",
                "fontStyle": ""
            }
        },
        {
            "name": "strings json",
            "scope": [
                "punctuation.support.type.property-name.end.json",
                "punctuation.support.type.property-name.begin.json"
            ],
            "settings": {
                "foreground": "#fe8170",
                "fontStyle": ""
            }
        },
        {
            "name": "types",
            "scope": [
                "entity."
            ],
            "settings": {
                "foreground": "#4eb0cb",
                "fontStyle": ""
            }
        },
        {
            "name": "types cpp",
            "scope": [
                "storage.type.built-in.cpp"
            ],
            "settings": {
                "foreground": "#4eb0cb",
                "fontStyle": ""
            }
        },
    ],
